{
  "name": "hentai",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "trustedDependencies": ["supabase"],
  "scripts": {
    "build": "remix vite:build",
    "dev": "node ./server.mjs",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "cross-env NODE_ENV=production node ./server.mjs",
    "typecheck": "tsc",
    "build-vercel-edge": "bash misc/vercel-edge/build.sh",
    "release-vercel-edge": "vercel deploy --prebuilt misc/vercel-edge --prod",
    "build-vercel-serverless": "bash misc/vercel-serverless/build.sh",
    "release-vercel-serverless-staging": "vercel deploy --prebuilt misc/vercel-serverless",
    "release-vercel-serverless": "vercel -e .env deploy --prebuilt misc/vercel-serverless --prod",
    "supabase:start": "supabase start",
    "gentypes": "supabase gen types typescript --db-url postgresql://postgres:postgres@127.0.0.1:54322/postgres > database.types.ts"
  },
  "dependencies": {
    "@hcaptcha/react-hcaptcha": "^1.9.3",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-context-menu": "^2.1.5",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@remix-run/express": "^2.4.1",
    "@remix-run/node": "^2.4.1",
    "@remix-run/react": "^2.4.1",
    "@supabase/supabase-js": "^2.39.1",
    "@types/nprogress": "^0.2.3",
    "bun": "^1.0.25",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cmdk": "^0.2.0",
    "express": "^4.18.2",
    "isbot": "^3.6.8",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.302.0",
    "morgan": "^1.10.0",
    "nprogress": "^0.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-use": "^17.4.2",
    "remix-themes": "^1.2.2",
    "supabase": "^1.131.4",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.8.9",
    "vite-plugin-commonjs": "^0.10.1",
    "zod": "^3.22.4",
    "zod-form-data": "^2.0.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.4.1",
    "@remix-run/dev": "^2.4.1",
    "@types/bun": "^1.0.3",
    "@types/express": "^4.17.20",
    "@types/morgan": "^1.9.9",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "typescript": "^5.1.6",
    "vite": "^5.0.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
